Size: this.$rem(0.7)\n          },\n          splitLine: {\n            show: true,\n            lineStyle: {\n              color: '#EFF3F6'\n            }\n          },\n          axisLine: {\n            show: true,\n            lineStyle: {\n              color: '#EFF3F6'\n            }\n          },\n          axisLabel: {\n            show: true,\n            margin: 20,\n            textStyle: {\n              color: '#A1A0AE',\n              fontSize: this.$rem(0.6)\n            }\n          },\n          axisTick: {\n            show: false,\n          },\n        }],\n        series: [\n          {\n            name: '光照',\n            type: 'line',\n            smooth: true, //是否平滑\n            showAllSymbol: true,\n            // symbol: 'image://./static/images/guang-circle.png',\n            symbol: 'circle',\n            showSymbol: false,\n            symbolSize: this.$rem(0.3),\n            lineStyle: {\n              normal: {\n                color: \"#54D8FF\",\n              },\n            },\n            label: {\n              show: true,\n              position: 'top',\n              textStyle: {\n                color: '#54D8FF',\n                fontSize: this.$rem(0.7)\n              }\n            },\n\n            itemStyle: {\n              color: \"#fff\",\n              borderColor: \"#54D8FF\",\n              borderWidth: this.$rem(0.2),\n            },\n            tooltip: {\n              show: true,\n            },\n            areaStyle: {\n              normal: {\n                color: new this.$echarts.graphic.LinearGradient(0, 0, 0, 1, [{\n                  offset: 0,\n                  color: 'rgba(84,216,255,1)'\n                },\n                  {\n                    offset: 1,\n                    color: 'rgba(255,255,255,0)'\n                  }\n                ], false),\n              }\n            },\n            data: [],\n          },\n        ]\n      },\n      //异步数据请求定时器挂载变量\n      delay: null,\n      //